<?xml version="1.0"?>

<PropertyList>

  <sim include="Nasal/views.xml">

    <author>BARANGER Emmanuel (3D/FDM)</author>
    <aircraft-version>0.1</aircraft-version>
    <status>Alpha</status>

    <fuel-fraction>1.0</fuel-fraction>

    <startup>
      <splash-texture>Aircraft/Velocity-XL/velocity-splash.rgb</splash-texture>
    </startup>

    <sound>
      <path archive="y">Aircraft/Velocity-XL/Sounds/velocity-sound.xml</path>
    </sound>

    <model>
      <path archive="y">Aircraft/Velocity-XL/Models/velocity.xml</path>
      <livery>
        <file type="string">default</file>
      </livery>
    </model>

    <multiplay>
      <chat_display>1</chat_display>
      <generic>
        <int type="int">0</int>
        <int type="int">0</int>
        <int type="int">0</int>
        <int type="int">0</int>
        <int type="int">0</int>
        <int type="int">0</int>
      </generic>
    </multiplay>

    <help>
      <title>Velocity-XL</title>
      <key>
        <name>d</name>
        <desc>Open/Close canopy</desc>
      </key>
      <line>______________________________________________</line>
    </help>

    <menubar include="Dialogs/velocity-menu.xml"/>

  </sim>

  <controls>
    <engines>
      <engine>
        <mixture archive="y"> 0.65 </mixture>
      </engine>
    </engines>
  </controls> 

  <input>
    <keyboard include="Nasal/velocity-keyboard.xml"/>
  </input>
 
  <nasal>
    <longez>
      <file>Aircraft/Velocity-XL/Nasal/doors.nas</file>
      <file>Aircraft/Velocity-XL/Nasal/light.nas</file>
<!--  <file>Aircraft/Velocity-XL/Nasal/velocity.nas</file> -->
      <file>Aircraft/Velocity-XL/Nasal/liveries.nas</file>
   </longez>
  </nasal>
  
</PropertyList>
