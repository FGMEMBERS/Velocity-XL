<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>throttle.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>anneau</object-name>
    <object-name>gaz</object-name>

    <!-- Objets transparents -->
  </animation>

  <animation>
    <type>translate</type>
    <object-name>gaz</object-name>
    <property>controls/engines/engine[0]/throttle</property>
    <interpolation>
      <entry><ind> 0 </ind><dep> 0.119 </dep></entry>
      <entry><ind> 1 </ind><dep> 0.000 </dep></entry>
    </interpolation>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>
