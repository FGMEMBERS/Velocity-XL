<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>pitch.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>anneau</object-name>
    <object-name>pitch</object-name>

    <!-- Objets transparents -->
  </animation>

  <animation>
    <type>translate</type>
    <object-name>pitch</object-name>
    <property>controls/engines/engine/propeller-pitch</property>
    <factor>-0.077</factor>  
    <interpolation>
      <entry><ind> 0 </ind><dep> 0.06 </dep></entry>
      <entry><ind> 1 </ind><dep> 0.00 </dep></entry>
    </interpolation>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>
