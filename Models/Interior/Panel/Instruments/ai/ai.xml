<?xml version="1.0"?>

<PropertyList>

  <path>ai.ac</path>

  <animation>
    <type>material</type>
    <object-name>Background</object-name>
    <object-name>Scale</object-name>
    <object-name>Float</object-name>
    <object-name>Markings</object-name>
    <emission>
      <red>   0.030 </red>
      <green> 0.015 </green>
      <blue>  0.005 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Background</object-name>
    <object-name>Scale</object-name>
    <object-name>Float</object-name>
    <property>/orientation/roll-deg</property>
    <factor>1.0</factor>
    <axis>
      <x>1.0</x>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>Float</object-name>
    <property>/orientation/pitch-deg</property>
    <factor>-0.0002</factor>
    <min-m>-0.1</min-m>
    <max-m>0.1</max-m>
    <axis>
      <z>1.0</z>
    </axis>
  </animation>

</PropertyList>
