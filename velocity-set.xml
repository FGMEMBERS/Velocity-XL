<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList include="velocity-base.xml">

  <sim>

    <description>Velocity XL (YASim)</description>
    <author>BARANGER Emmanuel (3D/FDM), Brian GLADDEN (3D), Gary R. NEELY and John LICHTENBERGER (FDM)</author>

    <flight-model archive="y">yasim</flight-model>
    <aero archive="y">velocity-yasim</aero>
    
    <rating>
      <FDM type="int">     2 </FDM>
      <systems type="int"> 2 </systems>
      <cockpit type="int"> 3 </cockpit>
      <model type="int">   4 </model>
    </rating>
   
  </sim>

  <consumables>
    <fuel>
      <tank n="0">
        <name type="string">Sump tank</name>
        <hidden type="bool">true</hidden>
      </tank>
      <tank n="1">
        <name type="string">Left wing tank</name>
      </tank>
      <tank n="2">
        <name type="string">Right wing tank</name>
      </tank>
    </fuel>
  </consumables>

</PropertyList>
